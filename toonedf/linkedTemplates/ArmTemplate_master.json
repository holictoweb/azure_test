{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "toonedf"
		},
		"azuredatalakegen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azuredatalakegen2'"
		},
		"azuresqldatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuresqldatabase'"
		},
		"onpremisefilesystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'onpremisefilesystem'"
		},
		"tooneadls2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'tooneadls2'"
		},
		"toonefilesystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'toonefilesystem'"
		},
		"vmSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'vmSqlServer'"
		},
		"azuredatalakegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://toonestorage02.dfs.core.windows.net"
		},
		"onpremisefilesystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "F:\\datafactoryDEMO"
		},
		"onpremisefilesystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "admin_orange"
		},
		"tooneadls2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://toonestorage02.dfs.core.windows.net"
		},
		"toonefilesystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "E:\\Data\\"
		},
		"toonefilesystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "admin_orange"
		},
		"trigger_toone_01_properties_pipeline_parameters_FolderPath_Source": {
			"type": "string",
			"defaultValue": "E:\\"
		},
		"trigger_toone_01_properties_pipeline_parameters_Directory_Source": {
			"type": "string",
			"defaultValue": "Data"
		},
		"trigger_toone_01_properties_pipeline_parameters_FolderPath_Destination": {
			"type": "string",
			"defaultValue": "toonedatafactory"
		},
		"trigger_toone_01_properties_pipeline_parameters_Directory_Destination": {
			"type": "string",
			"defaultValue": "migrationdata"
		},
		"trigger_toone_01_properties_pipeline_parameters_LastModified_From": {
			"type": "string",
			"defaultValue": "@trigger (). outputs.windowStartTime"
		},
		"trigger_toone_01_properties_pipeline_parameters_LastModified_To": {
			"type": "string",
			"defaultValue": "@trigger (). outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azuredatalakegen2_accountKey": {
						"value": "[parameters('azuredatalakegen2_accountKey')]"
					},
					"azuresqldatabase_connectionString": {
						"value": "[parameters('azuresqldatabase_connectionString')]"
					},
					"tooneadls2_accountKey": {
						"value": "[parameters('tooneadls2_accountKey')]"
					},
					"vmSqlServer_connectionString": {
						"value": "[parameters('vmSqlServer_connectionString')]"
					},
					"onpremisefilesystem_password": {
						"value": "[parameters('onpremisefilesystem_password')]"
					},
					"toonefilesystem_password": {
						"value": "[parameters('toonefilesystem_password')]"
					},
					"azuredatalakegen2_properties_typeProperties_url": {
						"value": "[parameters('azuredatalakegen2_properties_typeProperties_url')]"
					},
					"tooneadls2_properties_typeProperties_url": {
						"value": "[parameters('tooneadls2_properties_typeProperties_url')]"
					},
					"onpremisefilesystem_properties_typeProperties_host": {
						"value": "[parameters('onpremisefilesystem_properties_typeProperties_host')]"
					},
					"onpremisefilesystem_properties_typeProperties_userId": {
						"value": "[parameters('onpremisefilesystem_properties_typeProperties_userId')]"
					},
					"toonefilesystem_properties_typeProperties_host": {
						"value": "[parameters('toonefilesystem_properties_typeProperties_host')]"
					},
					"toonefilesystem_properties_typeProperties_userId": {
						"value": "[parameters('toonefilesystem_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger_toone_01_properties_pipeline_parameters_FolderPath_Source": {
						"value": "[parameters('trigger_toone_01_properties_pipeline_parameters_FolderPath_Source')]"
					},
					"trigger_toone_01_properties_pipeline_parameters_Directory_Source": {
						"value": "[parameters('trigger_toone_01_properties_pipeline_parameters_Directory_Source')]"
					},
					"trigger_toone_01_properties_pipeline_parameters_FolderPath_Destination": {
						"value": "[parameters('trigger_toone_01_properties_pipeline_parameters_FolderPath_Destination')]"
					},
					"trigger_toone_01_properties_pipeline_parameters_Directory_Destination": {
						"value": "[parameters('trigger_toone_01_properties_pipeline_parameters_Directory_Destination')]"
					},
					"trigger_toone_01_properties_pipeline_parameters_LastModified_From": {
						"value": "[parameters('trigger_toone_01_properties_pipeline_parameters_LastModified_From')]"
					},
					"trigger_toone_01_properties_pipeline_parameters_LastModified_To": {
						"value": "[parameters('trigger_toone_01_properties_pipeline_parameters_LastModified_To')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}